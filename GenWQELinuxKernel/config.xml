    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>H * * * *</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>pwd

# rm -rf linux linux-build
#git clone https://github.com/torvalds/linux

mkdir -p linux-build
make -C linux O=../linux-build defconfig
sed -i &apos;s/# CONFIG_GENWQE is not set/CONFIG_GENWQE=m\nCONFIG_GENWQE_PLATFORM_ERROR_RECOVERY=0/g&apos; \
	linux-build/.config
make -C linux O=../linux-build oldconfig
make -C linux O=../linux-build -j32

echo &quot;Check if genwqe_card.ko is nicely build ...&quot;
ls -l linux-build/drivers/misc/genwqe</command>
    </hudson.tasks.Shell>
  </builders>
